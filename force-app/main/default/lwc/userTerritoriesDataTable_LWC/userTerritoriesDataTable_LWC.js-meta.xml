<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata" fqn="travelApprovalPath_LWC">
    <apiVersion>49.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>User Territories Selection Lightning Web Component</masterLabel>
    <description>This is a &quot;User Territories Selection&quot; LWC demo component.</description>
    <targets>
        <target>lightning__RecordPage</target>
        <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__RecordPage">
            <property name="singleRowSelection" type="Boolean" label="Single Row Selection?" default="true"/>
            <supportedFormFactors>
                <supportedFormFactor type="Small" />
                <supportedFormFactor type="Large" />
            </supportedFormFactors>
        </targetConfig>
        <targetConfig targets="lightning__FlowScreen">
            <property name="recordId" type="String" label="Record Id"/>
            <property name="singleRowSelection" type="Boolean" label="Single Row Selection?" default="true"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>
